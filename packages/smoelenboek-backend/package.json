{
  "name": "smoelenboek-backend",
  "version": "5.0.0",
  "description": "API for smoelenboek",
  "main": "dist/index.js",
  "author": "Fabio Dijkshoorn <nouw@nouw.net>",
  "license": "MIT",
  "packageManager": "yarn@4.1.0",
  "scripts": {
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "typeorm": "typeorm-ts-node-commonjs",
    "create:migration": "yarn typeorm -d ./src/Database.ts migration:generate",
    "migrate": "yarn typeorm -d ./src/Database.ts migration:run",
    "compile": "rm -rf ./dist && tsc --project tsconfig.json",
    "dev": "tsnd --respawn --transpile-only --inspect --env-file ../../.env -- src/index.ts",
    "start": "node ./dist/index.js",
    "deploy": "./bin/deploy.sh",
    "execute": "ts-node ./src/Cli.ts",
    "schema:sync": "yarn typeorm -d ./src/Database.ts schema:sync"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/mailgen": "^2.0.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.1",
    "@types/nodemailer": "^6.4.9",
    "@types/passport": "^0",
    "@types/passport-jwt": "^4",
    "@types/passport-oauth2": "^1",
    "@types/yargs": "^17.0.29",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "ts-node-dev": "^2.0.0"
  },
  "dependencies": {
    "@decorators/di": "^2.0.0",
    "@decorators/express": "^2.9.3",
    "@decorators/socket": "^3.4.1",
    "@googleapis/drive": "^5.1.0",
    "@googleapis/sheets": "^4.0.2",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.3",
    "bull": "^4.11.3",
    "cors": "^2.8.5",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "google-auth-library": "^8.8.0",
    "google-spreadsheet": "^4.1.1",
    "jsonwebtoken": "^9.0.0",
    "lexical": "^0.11.1",
    "mailgen": "^2.0.27",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.3.0",
    "nodemailer": "^6.9.4",
    "oci-common": "^2.61.0",
    "oci-objectstorage": "^2.61.0",
    "oci-sdk": "^2.61.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-oauth2": "^1.8.0",
    "puppeteer": "^21.3.8",
    "reflect-metadata": "^0.1.13",
    "smoelenboek-types": "workspace:^",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "yargs": "^17.7.2"
  }
}
